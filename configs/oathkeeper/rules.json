[
    {
        "id": "maktaba-api",
        "upstream": {
            "url": "http://maktaba:8080"
        },
        "match": {
            "url": "http://localhost:4455/<{sources,notes,health,metrics}**>",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH"
            ]
        },
        "authenticators": [
            {
                "handler": "cookie_session"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [
            {
                "handler": "id_token"
            }
        ],
        "errors": [
            {
                "handler": "json"
            }
        ]
    }
]